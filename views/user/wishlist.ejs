    <!-- breadcrumb start-->
  

<!--================wishlist Area =================-->
  <section class="cart_area padding_top">
    <div class="container">
      <div class="cart_inner">
        <div class="table-responsive">
          <table class="table">
            <thead>
              <tr>
                <th scope="col">Product</th>
                <th scope="col">Price</th>
                <th scope="col">action</th>
              </tr>
            </thead>

            <tbody>
                <% if(noWishlist){ %>
                    <h6>no items</h6>
                <% }else{ %>    

            <% user.forEach((wishItem,index) => { %> 
              <tr>
                <td>
                  <div class="media">
                    <div class="d-flex">
                      <a href="/product/<%= wishItem.wishlist[0]._id %>">
                      <img src="<%= wishItem.wishlist[0].images[0] %>" alt="" width="140" height="120" />
                      </a>
                    </div>
                    <div class="media-body">
                      <h5><%= wishItem.wishlist[0].name %></h5>
                      <p><%= wishItem.wishlist[0].description %></p>
                    </div>
                  </div>
                </td>
                <td>
                  <h5>$<%= wishItem.wishlist[0].price %></h5>
                </td>
                <td>
                  <a href="/wishlist/remove/<%= wishItem.wishlist[0]._id %>" class="btn btn-danger">remove</a>
                </td>

            </tr>
            <% }) %>
            <% } %>
                
            </tbody>
          </table>
              
            
          <div class="checkout_btn_inner float-right">
            <a class="btn_1" href="/shop">Continue Shopping</a>
            <hr>
          </div>
            
        </div>
      </div>
  </section>
  
   

    
