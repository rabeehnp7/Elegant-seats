    <!-- product_list start-->
    <section class="product_list section_padding">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-12">
                    <div class="section_tittle text-center">
                        <h2>awesome <span>shop</span></h2>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <div class="product_list_slider owl-carousel">  
                        <div class="single_product_list_slider">
                            <div class="row align-items-center justify-content-between">
                            <% for (let i = 0; i < products.length && i < 8; i++) { %>
                                <% if (!products[i].softDeleted) { %>
                                <div class="col-lg-3 col-sm-6">
                                    <div class="single_product_item">
                                        <a href="/product/<%= products[i]._id %>">
                                            <img  src="<%= products[i].images[0] %>" alt="">
                                        </a>
                                        <div class="single_product_text">
                                            <h4><%= products[i].name %></h4>
                                            <h3>&#8377;<%= products[i].price %></h3>
                                            <a href="/addTowishlist/<%= products[i]._id %>" class="add_cart"><i class="ti-heart"></i></a>
                                        </div>
                                    </div>
                                </div>
                                <% } %>
                            <% } %> 
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            

        </div>
    </section>
                            
                                

                                            
