    <!-- banner part start-->
    <section class="banner_part">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-12">
                    
                    
                    <div class="banner_slider owl-carousel">

                        <% banner.forEach((bnr)=>{  %>
                            <div class="single_banner_slider <%= bnr.images[0] ? 'background-image1' : '' %>">
                                <style>
                                    .background-image1 {
                                     background-repeat: no-repeat;
                                    }
                                 
                                   @media (max-width: 991.98px) {
                                     .background-image1 {
                                        background-image: url('<%= bnr.images[0] %>');
                                     }
                                   }
                                </style>
                                 
                                <div class="row">
                                    <div class="col-lg-5 col-md-8">
                                        <div class="banner_text">
                                            <div class="banner_text_iner">
                                                
                                                <h1><%= bnr.heading %> </h1>
                                                                       
                                                <p><strong>Shop our wide selection of chairs and furniture in a variety of styles and finishes</strong></p>
                                                <a href="/shop" class="btn_2">Explore Shop</a>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="banner_img d-none d-lg-block">
                                        <img src="<%= bnr.images[0] %>" alt="">
                                    </div>
                                </div>
                            </div>
                        <% }) %>    
                    <% banner.forEach((bnr)=>{  %>
                        <div class="single_banner_slider <%= bnr.images[1] ? 'background-image2' : '' %>">
                            <style>
                                .background-image2 {
                                 background-repeat: no-repeat;
                                }
                             
                               @media (max-width: 991.98px) {
                                 .background-image2 {
                                    background-image: url('<%= bnr.images[1] %>');
                                 }
                               }
                            </style>
                            <div class="row">
                                <div class="col-lg-5 col-md-8">
                                    <div class="banner_text">
                                        <div class="banner_text_iner">
                                            
                                            <h1><%= bnr.heading %> </h1>
                                                                   
                                            <p><strong>Shop our wide selection of chairs and furniture in a variety of styles and finishes</strong></p>
                                            <a href="/shop" class="btn_2">Explore Shop</a>
                                        </div>
                                    </div>
                                </div>
                                <div class="banner_img d-none d-lg-block">
                                    <img src="<%= bnr.images[1] %>" alt="">
                                </div>
                            </div>
                        </div>
                    <% }) %>    
                    <% banner.forEach((bnr)=>{  %>
                        <div class="single_banner_slider <%= bnr.images[2] ? 'background-image3' : '' %>">
                            <style>
                                .background-image3 {
                                 background-repeat: no-repeat;
                                }
                             
                               @media (max-width: 991.98px) {
                                 .background-image3 {
                                    background-image: url('<%= bnr.images[2] %>');
                                 }
                               }
                            </style>
                            <div class="row">
                                <div class="col-lg-5 col-md-8">
                                    <div class="banner_text">
                                        <div class="banner_text_iner">
                                            
                                            <h1><%= bnr.heading %> </h1>
                                                                   
                                            <p><strong>Shop our wide selection of chairs and furniture in a variety of styles and finishes</strong></p>
                                            <a href="/shop" class="btn_2">Explore Shop</a>
                                        </div>
                                    </div>
                                </div>
                                <div class="banner_img d-none d-lg-block">
                                    <img src="<%= bnr.images[2] %>" alt="">
                                </div>
                            </div>
                        </div>
                    <% }) %>    
                        
                        
                    </div>
                    <div class="slider-counter"></div>
                </div>
            </div>
        </div>
    </section>
    <!-- banner part start-->

    <!-- feature_part start-->
    <section class="feature_part padding_top">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-8">
                    <div class="section_tittle text-center">
                        <h2>Featured Category</h2>
                    </div>
                </div>
            </div>
            <div class="row align-items-center justify-content-between">
                <% categories.forEach(function(category) { %>
                <% if (!category.isDestroyed) { %>
                   
                <div class="col-lg-5 col-sm-6 mt-5">
                    <div class="single_feature_post_text mt-5">
                        <p><%=category.name%></p>
                        <a href="/shop?category=<%= category.name %>" class="feature_btn">EXPLORE NOW <i class="fas fa-play"></i></a>
                        <img src="<%= category.image %>" alt="">
                    </div>
                </div>
                <% } %>
                <% }); %>  
                   
                        
                    
